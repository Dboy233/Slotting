[
  {
    "classPath": "com.dboy.slotting.example.MainActivity",
    "entryPoints": [
      {
        "methodName": "onCancel",
        "event": "cancelLogin"
      },
      {
        "methodName": "loginError",
        "event": "user_login_failure_,${errorName}"
      },
      {
        "methodName": "loginSuccess",
        "event": "user_login_success"
      },
      {
        "methodName": "onCheckChange",
        "event": "协议:,${agree},_check:,${isChecked}"
      },
      {
        "methodName": "onDestroy",
        "eventMap": {
          "msg": "退出",
          "business": "${this.businessName}"
        }
      }
    ]
  },
  {
    "classPath": "com.dboy.slotting.example.InfoActivity",
    "entryPoints": [
      {
        "methodName": "showInfo",
        "eventMap": {
          "msg": "用户信息",
          "password": "${password}",
          "name": "${name}"
        }
      },
      {
        "methodName": "closePager",
        "event": "关闭详情页"
      }
    ]
  }
]